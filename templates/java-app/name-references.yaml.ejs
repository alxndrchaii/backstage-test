nameReference:
- kind: Deployment
  fieldSpecs:
  - path: spec/targetRef/name
    group: flagger.app
    kind: Canary

- kind: HorizontalPodAutoscaler
  fieldSpecs:
  - path: spec/autoscalerRef/name
    group: flagger.app
    kind: Canary